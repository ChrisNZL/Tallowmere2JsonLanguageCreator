{
	"name": "Tallowmere 2 JSON Language Creator",
	"version": "0.0.0",
	"manifest_version": 2,
	"description": "Converts a Google Spreadsheet into a JSON file for use with Tallowmere 2.",
	"icons": {
		"246": "TallowmereBust.png"
	},
	"minimum_chrome_version": "27",
	"offline_enabled": false,
	"content_scripts": [
    {
      "matches": ["http://127.0.0.1/*Tallowmere2LanguageFiles/*.html"],
      "js": ["jquery-2.1.1.min.js", "Converter.js"],
      "run_at": "document_start"
    }
  ]
}